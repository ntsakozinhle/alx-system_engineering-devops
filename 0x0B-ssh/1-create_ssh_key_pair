#!/usr/bin/env bash
# This script creates an RSA key pair with the name 'school' and protected by passphrase 'betty'

# Generates the RSA key pair with 4096 bits and the passphrase 'betty'
ssh-keygen -t rsa -b 4096 -C "Generated by script" -f ~/.ssh/school -N "betty"

# Display success message
echo "RSA key pair successfully created."
